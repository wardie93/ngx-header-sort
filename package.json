{
    "name": "ngx-header-sort-app",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve -o",
        "build": "ng build",
        "serve": "ng serve",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "version:patch": "cd projects/ngx-header-sort && npm version patch",
        "version:minor": "cd projects/ngx-header-sort && npm version patch",
        "version:major": "cd projects/ngx-header-sort && npm version patch",
        "build:lib": "ng build --project=ngx-header-sort",
        "build:lib:prod": "ng build --project=ngx-header-sort --configuration production",
        "watch:lib": "ng build --project=ngx-header-sort --watch",
        "watch:lib:prod": "ng build --project=ngx-header-sort --watch --configuration production",
        "publish": "cd dist/ngx-header-sort && npm publish"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~12.0.0",
        "@angular/common": "~12.0.0",
        "@angular/compiler": "~12.0.0",
        "@angular/core": "~12.0.0",
        "@angular/forms": "~12.0.0",
        "@angular/platform-browser": "~12.0.0",
        "@angular/platform-browser-dynamic": "~12.0.0",
        "@angular/router": "~12.0.0",
        "@fortawesome/fontawesome-free": "^5.15.3",
        "rxjs": "~6.6.0",
        "tslib": "^2.1.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~12.0.0",
        "@angular/cli": "~12.0.0",
        "@angular/compiler-cli": "~12.0.0",
        "@types/jasmine": "~3.6.0",
        "@types/node": "^12.11.1",
        "jasmine-core": "~3.7.0",
        "karma": "~6.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.0.3",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "ng-packagr": "^12.0.0",
        "typescript": "~4.2.3"
    }
